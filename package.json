{
  "name": "raid-cd-planner",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "^18",
    "npm": "^9"
  },
  "engineStrict": true,
  "scripts": {
    "start:app": "vite",
    "start:server": "nodemon --ignore src/frontend/ src/backend/main.ts",
    "test": "node --loader tsx --test src/backend/**/*.test.ts",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "build:app": "tsc && vite build",
    "build:server": "tsc --p src/backend/tsconfig.json",
    "build": "npm run build:server",
    "start": "node build/src/backend/main.js"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "cors": "^2.8.5",
    "d3": "^7.8.5",
    "date-fns": "^2.17.0",
    "dotenv": "^16.3.1",
    "express": "^4.17.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.20.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.16",
    "@types/d3": "^7.4.3",
    "@types/debounce": "^1.2.1",
    "@types/express": "^4.17.21",
    "@types/react": "18.2.17",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "msw": "^2.0.10",
    "nodemon": "^3.0.1",
    "sass": "^1.50.0",
    "ts-node": "^10.9.1",
    "tsx": "^4.2.0",
    "typescript": "^5.2.0",
    "vite": "^5.0.10"
  }
}
